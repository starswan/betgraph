<h1>New menu_path</h1>

<% form_for(@menu_path) do |f| %>
  <%= render "shared/error_messages", :target => @menu_path %>

  <p>
    <%= f.label :active %><br />
    <%= f.check_box :active %>
  </p>
  <p>
    <%= f.label :depth %><br />
    <%= f.text_field :depth %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :parent_path %><br />
    <%= f.text_field :parent_path %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', menu_paths_path %>
