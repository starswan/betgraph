<tr>
  <td><%= fixture.kickofftime.to_fs(:time_without_secs) %></td>
  <td><%= fixture.division.name %></td>
  <td><%= render 'team', team: fixture.hometeam, position: league_table.position(fixture.hometeam) %></td>
  <td><%= render 'team', team: fixture.awayteam, position: league_table.position(fixture.awayteam) %></td>
  <td><%= fixture.bet_markets_count %></td>
  <td><%= fixture.market_prices_count %></td>
</tr>
